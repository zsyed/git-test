@model eManager.Domain.Department

@{
    ViewBag.Title = "Detail";
}

<h2>@Model.Name</h2>

@Html.ActionLink("Create an Employee", "Create", "Employee", new { DepartmentId = @Model.Id} , null)

<table>
    <tr>
        <th>Name</th>
        <th></th>
    </tr>

    @foreach (var empl in Model.Employees)
    {
        <tr>
            <td>@empl.Name</td>
            <td>

            </td>
        </tr>
    }
</table>
